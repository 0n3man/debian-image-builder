STATIC="qemu-arm-static"

# device tree blob
DTB="sun8i-h3-nanopi-m1"
FAMILY="allwinner"

# default config
LINUX_DEFCONFIG="allwinner-sun8i_defconfig"
UBOOT_DEFCONFIG="nanopi_m1_defconfig"
IMG_EXP="${DTB}-*.img*"
