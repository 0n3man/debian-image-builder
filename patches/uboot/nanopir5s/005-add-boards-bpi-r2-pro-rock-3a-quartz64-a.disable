+
+&usb_host1_ehci {
+	vbus-supply = <&vcc5v0_usb20_host>;
+};
+
+&usb_host1_ohci {
+	vbus-supply = <&vcc5v0_usb20_host>;
+};
+
+&uart2 {
+	clock-frequency = <24000000>;
+	u-boot,dm-spl;
+	status = "okay";
+};
diff -Naur a/arch/arm/dts/rk3568-bpi-r2-pro.dts b/arch/arm/dts/rk3568-bpi-r2-pro.dts
