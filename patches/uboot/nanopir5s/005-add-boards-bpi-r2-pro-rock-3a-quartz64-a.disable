+&usb_host0_ehci {
+	vbus-supply = <&vcc5v0_usb_host>;
+};
+
+&usb_host0_ohci {
+	vbus-supply = <&vcc5v0_usb_host>;
+};
+
+&usb_host1_ehci {
+	vbus-supply = <&vcc5v0_usb_host>;
+};
+
+&usb_host1_ohci {
+	vbus-supply = <&vcc5v0_usb_host>;
+};
+
+&uart2 {
+	clock-frequency = <24000000>;
+	u-boot,dm-spl;
+	status = "okay";
+};
