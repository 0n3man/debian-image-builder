+	tx-fifo-depth = <4096>;
+};
+
 &cec_AO {
 	pinctrl-0 = <&cec_ao_a_h_pins>;
 	pinctrl-names = "default";
@@ -235,7 +248,7 @@ &sd_emmc_a {
 
 	bus-width = <4>;
