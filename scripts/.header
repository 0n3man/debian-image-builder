#!/bin/bash

if [[ `command -v figlet` ]]; then :; else sudo apt install -y figlet; echo ""; fi

GRN="\e[0;32m"
PNK="\e[0;35m"
TXT="\033[1m"
FIN="\e[0m"

BUILDER=`figlet -f term -l "Debian Image Builder"`
BRANCH=`git branch`

echo -en "${TXT}${BUILDER}${FIN} ${PNK}[${FIN}${GRN}${BRANCH}${FIN}${PNK}]${FIN}"
echo ""

exit 0
